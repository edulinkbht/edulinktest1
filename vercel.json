{
  "rewrites": [
    {
      "source": "/",
      "destination": "/home"
    },
    {
      "source": "/profile/:username*",
      "destination": "/home"
    },
    {
      "source": "/feed",
      "destination": "/home"
    },
    {
      "source": "/search",
      "destination": "/home"
    },
    {
      "source": "/notifications",
      "destination": "/home"
    },
    {
      "source": "/(.*)",
      "destination": "/404.html"
    }
  ]
}
